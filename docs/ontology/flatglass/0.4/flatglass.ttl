xsd:gMonthYear a rdfs:Datatype .

<https://w3c-lbd-cg.github.io/bot#Zone> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <https://w3c-lbd-cg.github.io/bot#containsElement> ;
            owl:someValuesFrom :BuiltElement ] .

<https://w3c-lbd-cg.github.io/bot#containsElement> a owl:ObjectProperty .

:BuiltElementType a owl:Class .

:reuseParameter a owl:ObjectProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :BuiltElement :BuiltElementBatch ) ] .

:DismountingDirection a owl:Class ;
    rdfs:subClassOf :DismountingParameter .

:LigthTransmissionLevel a owl:Class ;
    rdfs:subClassOf :StructuralParameter ;
    owl:equivalentClass [ a owl:Class ;
            owl:oneOf ( :Above63Perc :Below63Perc ) ] .

:hasStructure a owl:ObjectProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :BuiltElement :BuiltElementBatch ) ] ;
    rdfs:range :StructuralParameter ;
    rdfs:subPropertyOf :reuseParameter .

:structuralValue a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :BuiltElement :BuiltElementBatch ) ] .

:Condition a owl:Class ;
    rdfs:subClassOf :StructuralParameter ;
    owl:equivalentClass [ a owl:Class ;
            owl:oneOf ( :fair :good :poor ) ] .

:SoundReductionLevel a owl:Class ;
    rdfs:subClassOf :StructuralParameter ;
    owl:equivalentClass [ a owl:Class ;
            owl:oneOf ( :level30 :level35 :level37 :level40 ) ] .

:StructuralParameter a owl:Class .

:U-value a owl:Class ;
    rdfs:subClassOf :StructuralParameter ;
    owl:equivalentClass [ a owl:Class ;
            owl:oneOf ( :0point7 :1point1 :1point8 :2point7 ) ] .

:WindowCompositionType a owl:Class ;
    rdfs:subClassOf :StructuralParameter .

:Material a owl:Class .

:WindowBatch a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom :Window ;
            owl:onProperty :hasBatchMember ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:int ;
            owl:onProperty :numberOfWindows ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :BuiltElementBatch .

:WindowFrame a owl:Class ;
    rdfs:comment "The frame or lining of a window."@en ;
    rdfs:subClassOf :BuiltElement .

:BuiltElementBatch a owl:Class ;
    rdfs:label "Built Element Batch"@en ;
    rdfs:comment "A set of building elements that contains only the same type of element, e.g. a set of windows with equal properties."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :batchIdentifier ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty :description ;
            owl:someValuesFrom xsd:string ] .

:BuiltElement a owl:Class ;
    rdfs:seeAlso "https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/class/IfcBuiltElement",
        "https://w3c-lbd-cg.github.io/bot/#Element" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :description ;
            owl:someValuesFrom xsd:string ] .

:recycleParameterValue a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :BuiltElement :BuiltElementBatch ) ] .

:WindowGlass a owl:Class ;
    rdfs:comment "The glass panel that is part of a window."@en ;
    rdfs:subClassOf :BuiltElement .

